{
  "name": "@carnesen/home-page",
  "description": "Source code for www.carnesen.com",
  "version": "0.0.0",
  "scripts": {
    "build": "node scripts/build.js && cross-env NODE_ENV=production webpack --bail",
    "build:watch": "node scripts/build.js && webpack-dev-server --content-base dist/",
    "lint": "eslint src webpack.config.babel.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run build && npm run test:unit",
    "test:unit": "mocha --require .before-browser-tests.js src/**/*-test.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@carnesen/checks": "0.0.1",
    "@carnesen/util": "0.0.7",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "cross-env": "3.1.2",
    "enzyme": "2.4.1",
    "eslint": "3.7.1",
    "eslint-config-carnesen": "1.0.4",
    "fs-extra": "0.30.0",
    "material-ui": "0.16.0",
    "mocha": "3.1.2",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-tap-event-plugin": "1.0.0",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  },
  "files": [
    "dist",
    "lib",
    "public"
  ],
  "author": {
    "name": "Chris Arnesen",
    "email": "chris.arnesen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carnesen/home-page.git"
  },
  "bugs": {
    "url": "https://github.com/carnesen/home-page/issues"
  },
  "homepage": "https://github.com/carnesen/home-page",
  "license": "MIT"
}
