{
  "name": "@carnesen/www",
  "description": "Source code for www.carnesen.com",
  "version": "0.0.4",
  "scripts": {
    "build": "next build && next export -o dist && node scripts/build.js",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
