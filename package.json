    "postversion": "npm publish"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {},
  "dependencies": {
    "@carnesen/styles": "0.0.1",
    "@zeit/next-css": "1.0.1",
    "enzyme": "3.8.0",
    "eslint": "5.12.0",
    "eslint-config-carnesen": "1.3.0",
    "fs-extra": "^7.0.1",
    "material-ui": "0.20.2",
    "next": "7.0.2",
    "next-fonts": "0.16.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "roboto-fontface": "0.10.0"
  }
}
